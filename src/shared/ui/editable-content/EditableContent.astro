---
interface Props {
  class?: string
  children: any
}

const { class: className, ...props } = Astro.props as Props

export { EditableContent }
---

<section
  class:list={['editable-content', className]}
  {...props}
>
  <slot />
</section>

<style is:global>
  @import './editable-content.scss';
</style>
